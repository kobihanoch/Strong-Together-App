{
  "cli": {
    "version": ">= 16.2.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": { "APP_PROFILE": "development" }
    },
    "preview": {
      "distribution": "internal",
      "env": { "APP_PROFILE": "development" }
    },
    "production": {
      "autoIncrement": true,
      "ios": { "image": "latest" },
      "env": { "APP_PROFILE": "production" }
    }
  },
  "submit": {
    "production": {}
  }
}
